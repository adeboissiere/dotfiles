export EDITOR="nvim"

# Oh my zsh
export ZSH="$HOME/.oh-my-zsh"
plugins=(
	git
	zsh-autosuggestions
	zsh-syntax-highlighting
  zsh-vi-mode
)
source $ZSH/oh-my-zsh.sh

# Oh my posh
eval "$(oh-my-posh init zsh --config ~/.config/oh-my-posh/config.toml)"

# Better ls
alias ls='lsd'

# Better cd
eval "$(zoxide init zsh)"
alias cd='z'

# Secrets
if [ -f ~/.secrets ]; then
  source ~/.secrets 
fi
